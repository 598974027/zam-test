#spring.rabbitmq.addresses=10.10.20.57:5672,10.10.20.58:5672,10.10.20.59:5672
spring.rabbitmq.username=rabbitmq
spring.rabbitmq.password=rabbitmq
spring.rabbitmq.virtual-host=/
#rabbitmq重连时间间隔(秒)
spring.rabbitmq.retry.initial-interval=10
#clb->dgs
clb.rabbitmq.todgs.exchangeName=exchange.broadcast2post.${intest.group}.${intest.actor}.nocmd
clb.rabbitmq.todgs.exchangeType=fanout
#clb->cmd
clb.rabbitmq.tocmd.exchangeName=exchange.broadcast2post.${intest.group}.${intest.actor}.cmd
clb.rabbitmq.tocmd.exchangeType=fanout
#clb->dss
clb.rabbitmq.todss.exchangeName=exchange.broadcast2post.${intest.group}.${intest.actor}.dss
clb.rabbitmq.todss.exchangeType=fanout
#dgs、cmd->clb
clb.rabbitmq.toclb.exchangeName=exchange.receivefrompost.${intest.group}.${intest.actor}.${intest.info}
clb.rabbitmq.toclb.exchangeType=fanout
clb.rabbitmq.toclb.queueName=queue.receivefrompost.${intest.group}.${intest.actor}.${intest.info}
clb.rabbitmq.toclb.routingKey=10
#队列绑定消费者个数
clb.rabbitmq.toclb.consumers=1
clb.rabbitmq.toclb.prefetch=1000
#redis单机配置
spring.redis.host=121.36.96.172
