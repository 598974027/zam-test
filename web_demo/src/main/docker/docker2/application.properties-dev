#服务组信息
intest.group=n60
intest.actor=clb
intest.info=1
#注册地址
register.httpUrl=http://113.57.170.58:63044/configapi/test
#clb节点名称
clb.name=CLB.N60
#clb版本配置
clb.version=1
#是否启用TLS双向认证
clb.tls=0
#读写解析线程数
clb.send.dgs.thread=2
clb.send.thread=1
clb.send.tbox.thread=1
#阻塞队列大小
queue.todgs=100000
queue.tosend=100000
queue.totbox=100000
#web端口配置
#server.port=${random.int[1000,8000]}
server.port=6419
#netty配置
